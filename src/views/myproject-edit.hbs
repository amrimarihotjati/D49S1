    <!-- Navbar -->
    <div class="container">
        <h1>Submit Project</h1>
        <div class="container-form">
            <form method="post" action="{{@index}}">
                <div class="mb-3">
                    <label for="input-name" class="form-label">Project Name</label>
                    <input type="text" class="form-control" id="input-project-name" name="projectName" aria-describedby="emailHelp" value="{{project.projectName}}" required>
                </div>
                <div class="mb-3">
                <div class="row g-3">
                        <div class="col">
                            <label for="start-date">Start Date</label>
                            <input type="date" class="form-control" name="startDate" id="input-start-date" value="{{project.startDate}}">
                        </div>
                        <div class="col">
                            <label for="end-date">End Date</label>
                            <input type="date" class="form-control" name="endDate" id="input-end-date" value="{{project.endDate}}">
                        </div>
                </div>
                <div class="mb-3">
                    <label for="input-message" class="form-label">Description</label>
                    <textarea class="form-control" id="input-description" name="projectDesc" rows="7" required>{{project.projectDesc}}</textarea>
                </div>
                <div class="mb-3">
                    <label>Technologies</label>
                    <div class="checklist-technologies">
                        <div class="col-tech">
                            <div class="checkbox-tech">
                                <input type="checkbox" id="check-nodejs" name="nodeJS" value="{{project.nodeJS}}"/>
                                <label>
                                    <i class="fa-brands fa-node-js fa-lg"></i>
                                    NodeJS
                                </label>
                            </div>
                            <div class="checkbox-tech">
                                <input type="checkbox" id="check-javascript" name="javaScript" value="{{project.javaScript}}" />
                                <label>
                                    <i class="fa-brands fa-js fa-lg"></i>
                                    JavaScript
                                </label>
                            </div>
                        </div>
                        <div class="col-tech">
                            <div class="checkbox-tech">
                                <input type="checkbox" id="check-reactjs" name="reactJS"/>
                                <label>
                                    <i class="fa-brands fa-react fa-lg"></i>
                                    ReactJS
                                </label>
                            </div>
                            <div class="checkbox-tech">
                                <input type="checkbox" id="check-html5" name="HTML"/>
                                <label>
                                    <i class="fa-brands fa-html5 fa-lg"></i>
                                    HTML
                                </label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="mb-3">
                    <label for="input-message" class="form-label">Upload Image</label>
                    <div class="input-group mb-3">
                        <input type="file" class="form-control" id="upload-image" name="image">
                        <label class="input-group-text" for="inputGroupFile02">Upload</label>
                    </div>
                </div>
                <div class="submit-position">
                    <button type="submit" class="btn btn-primary" >Submit</button>
                    {{!-- onclick="addMyProject()" --}}
                </div>
            </form>
        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    {{!-- <script src="js/myproject.js"></script> --}}
    <script src="index.js"></script>
